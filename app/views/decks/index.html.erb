<!-- <script data-main="js/app" src="js/require.js"></script> -->
<script src="/assets/app.js"></script>
<div class="card-portal">
  <div class="card-types"></div>
  <div class="card-sets">
    <div id="profile" name="profile">
      <label>Enter Name Before Proceeding<input type="text" placeholder="Enter Name of Your Deck" /></label>
      <button type="submit">Save</button>
    </div>
    <div id="decks" name="decks">
      <select class="deckSelection">
        <option selected>Loading Card Sets...</option>
      </select>
      <select id='types' name='display'>
        <option value="cmc" selected>CMC</option>
        <option value="colors">Colors</option>
        <option value="types">Types</option>
      </select>
      <button>Save Set</button>
    </div>
  </div>
  <div class="sub-set">
    <div class='pictures'></div>
  </div>
  <div id="visualizer">
    <div class="img"></div>
    <div class="card-placeholder">
      <div id='sideboard'>

        <h1>{{}}</h1>
      </div>
    </div>
    <div id="player">
        
    </div>
  </div>
  <script type="text/template" id="hello">
      <div> {{words}} </div>
  </script>
</div>